{
  "name": "alfabit-design-system",
  "version": "0.1.0",
  "private": true,
  "description": "A comprehensive React component library for building consistent and accessible user interfaces",
  "author": "ESousa97",
  "repository": {
    "type": "git",
    "url": "https://github.com/ESousa97/DesignSystem"
  },
  "keywords": [
    "react",
    "design-system",
    "components",
    "ui",
    "tailwindcss",
    "typescript",
    "storybook"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "typecheck": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "test": "test-storybook",
    "test:ci": "concurrently -k -s first -n SB,TEST -c magenta,blue \"http-server storybook-static --port 6006 --silent\" \"wait-on tcp:127.0.0.1:6006 && test-storybook --url http://localhost:6006\"",
    "audit": "npm audit --audit-level=moderate",
    "audit:fix": "npm audit fix",
    "prepare": "echo 'Hooks ready'",
    "precommit": "npm run lint && npm run format:check && npm run typecheck"
  },
  "dependencies": {
    "next": "14.2.35",
    "react": "^18",
    "react-dom": "^18",
    "react-icons": "^5.5.0"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^1.4.0",
    "@storybook/addon-essentials": "8.6.14",
    "@storybook/addon-interactions": "8.6.14",
    "@storybook/addon-links": "8.1.2",
    "@storybook/addon-onboarding": "8.1.2",
    "@storybook/blocks": "8.6.14",
    "@storybook/nextjs": "8.1.2",
    "@storybook/react": "8.1.2",
    "@storybook/test": "8.6.15",
    "@storybook/test-runner": "^0.24.2",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "concurrently": "^9.2.1",
    "eslint": "^8",
    "eslint-config-next": "14.2.35",
    "eslint-plugin-storybook": "^0.8.0",
    "http-server": "^14.1.1",
    "postcss": "^8",
    "prettier": "^3.2.0",
    "prettier-plugin-tailwindcss": "^0.7.2",
    "storybook": "8.1.2",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "wait-on": "^9.0.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "browserslist": [
    ">0.3%",
    "not dead",
    "not op_mini all"
  ],
  "overrides": {
    "markdown-to-jsx": "7.7.4"
  }
}
